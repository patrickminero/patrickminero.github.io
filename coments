<script>
        //mobile navigation bar script
        function toggleMenu(){
            document.getElementById("navBar").classList.toggle("buttons_out");
            document.getElementById("buttonContainer").classList.toggle("buttonContainerClosed");
        }

    </script>
    <script src="https://unpkg.com/aos@next/dist/aos.js"></script>
    <script>
      AOS.init();
    </script>


